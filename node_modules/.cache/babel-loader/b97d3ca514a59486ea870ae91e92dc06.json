).then(resp => {\r\n                    this.page()\r\n                    this.updateDialogVisible = false;\r\n                })\r\n            },\r\n            updateView(id) {\r\n                GoodsTypeApi.get(id).then(resp => {\r\n                    this.goodsType = resp.data.data\r\n                    this.updateDialogVisible = true\r\n                })\r\n            },\r\n            addView() {\r\n                this.dialogVisible = true;\r\n            },\r\n            save() {\r\n                GoodsTypeApi.add(this.supplier).then(resp => {\r\n                    this.dialogVisible = false;\r\n                    this.page()\r\n                })\r\n            },\r\n            deleteData(id) {\r\n                GoodsTypeApi.deleteData(id).then(resp => {\r\n                    if (resp.data.code == 200) {\r\n                        this.$message.success(\"删除成功\");\r\n                        this.page()\r\n                    } else {\r\n                        this.$message.error(resp.data.msg);\r\n                    }\r\n                })\r\n            },\r\n            page() {\r\n                GoodsTypeApi.page(this.current, this.size).then(resp => {\r\n                    this.data = resp.data.data.records;\r\n                    this.total = resp.data.data.total;\r\n                })\r\n            },\r\n\r\n            handleSizeChange(size) {\r\n                this.size = size;\r\n                this.page();\r\n            },\r\n            handleCurrentChange(current = 1) {\r\n                this.current = current;\r\n                this.page();\r\n            },\r\n        },\r\n        created() {\r\n            this.page()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}