{"ast":null,"code":"import GoodsTypeApi from \"@/api/goodsType/GoodsTypeApi\";\nexport default {\n  name: \"GoodType\",\n\n  data() {\n    return {\n      data: [],\n      // 分页数据\n      total: 10,\n      // 总记录数量\n      size: 5,\n      // 一页几条\n      current: 1,\n      dialogVisible: false,\n      goodsType: {\n        id: null,\n        name: ''\n      },\n      updateDialogVisible: false\n    };\n  },\n\n  methods: {\n    update() {\n      GoodsTypeApi.update(this.supplier).then(resp => {\n        this.page();\n        this.updateDialogVisible = false;\n      });\n    },\n\n    updateView(id) {\n      GoodsTypeApi.get(id).then(resp => {\n        this.goodsType = resp.data.data;\n        this.updateDialogVisible = true;\n      });\n    },\n\n    addView() {\n      this.dialogVisible = true;\n    },\n\n    save() {\n      GoodsTypeApi.add(this.supplier).then(resp => {\n        this.dialogVisible = false;\n        this.page();\n      });\n    },\n\n    deleteData(id) {\n      GoodsTypeApi.deleteData(id).then(resp => {\n        if (resp.data.code == 200) {\n          this.$message.success(\"删除成功\");\n          this.page();\n        } else {\n          this.$message.error(resp.data.msg);\n        }\n      });\n    },\n\n    page() {\n      GoodsTypeApi.page(this.current, this.size).then(resp => {\n        this.data = resp.data.data.records;\n        this.total = resp.data.data.total;\n      });\n    },\n\n    handleSizeChange(size) {\n      this.size = size;\n      this.page();\n    },\n\n    handleCurrentChange(current = 1) {\n      this.current = current;\n      this.page();\n    }\n\n  },\n\n  created() {\n    this.page();\n  }\n\n};","map":{"version":3,"mappings":"AAuEA;AAEA;EACAA,gBADA;;EAEAC;IACA;MACAA,QADA;MACA;MACAC,SAFA;MAEA;MACAC,OAHA;MAGA;MACAC,UAJA;MAKAC,oBALA;MAMAC;QACAC,QADA;QAEAP;MAFA,CANA;MAUAQ;IAVA;EAYA,CAfA;;EAgBAC;IACAC;MACAC;QACA;QACA;MACA,CAHA;IAIA,CANA;;IAOAC;MACAD;QACA;QACA;MACA,CAHA;IAIA,CAZA;;IAaAE;MACA;IACA,CAfA;;IAgBAC;MACAH;QACA;QACA;MACA,C