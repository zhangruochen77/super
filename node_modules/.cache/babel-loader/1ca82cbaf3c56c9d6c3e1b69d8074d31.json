{"ast":null,"code":"import adminApi from '@/api/admin/Admin';\nexport default {\n  name: \"Header\",\n\n  data() {\n    return {\n      personInfoVisible: false,\n      passwordVisible: false,\n      personAdmin: {\n        id: '',\n        name: '',\n        roleId: '',\n        username: '',\n        img: '',\n        phone: ''\n      },\n      updateAdmin: {\n        id: '',\n        name: '',\n        username: '',\n        img: '',\n        phone: ''\n      }\n    };\n  },\n\n  created() {\n    this.getPersonInfo();\n  },\n\n  methods: {\n    getPersonInfo() {\n      // 获取用户的个人信息\n      adminApi.getPersonInfo().then(resp => {\n        this.personAdmin = resp.data.data;\n      });\n    },\n\n    handleCommand(command) {\n      // 处理命令操作\n      if ('upInfo' === command) {\n        this.updateAdminInfo();\n      } else if ('upPass' === command) {\n        this.updatePassword();\n      } else if ('logout' === command) {\n        this.logout();\n      }\n    },\n\n    updateAdminInfo() {\n      // 更新用户信息\n      this.updateAdmin.id = this.personAdmin.id;\n      this.updateAdmin.img = this.personAdmin.img;\n      this.updateAdmin.name = this.personAdmin.name;\n      this.updateAdmin.phone = this.personAdmin.phone;\n      this.updateAdmin.username = this.personAdmin.username;\n      this.personInfoVisible = true;\n    },\n\n    updatePassword() {\n      // 更新密码\n      this.passwordVisible = true;\n    },\n\n    logout() {\n      // 退出登录\n      adminApi.logout();\n      window.sessionStorage.clear(); // 清空 token 信息\n\n      this.$router.replace('/');\n      this.$message.success('退出登录');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyDA;AAEA;EACAA,cADA;;EAEAC;IACA;MACAC,wBADA;MAEAC,sBAFA;MAGAC;QACAC,MADA;QAEAL,QAFA;QAGAM,UAHA;QAIAC,YAJA;QAKAC,OALA;QAMAC;MANA,CAHA;MAWAC;QACAL,MADA;QAEAL,QAFA;QAGAO,YAHA;QAIAC,OAJA;QAKAC;MALA;IAXA;EAmBA,CAtBA;;EAuBAE;IACA;EACA,CAzBA;;EA0BAC;IACAC;MAAA;MACAC;QACA;MACA,CAFA;IAGA,CALA;;IAMAC;MAAA;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA,CAFA,