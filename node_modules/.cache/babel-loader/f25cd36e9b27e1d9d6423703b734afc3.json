ton type=\"info\">更新</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            label=\"删除\"\r\n                            width=\"200\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button type=\"danger\" @click=\"deleteData(scope.row.id)\">删除</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </template>\r\n        </div>\r\n        <div>\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"current\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"size\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n\r\n\r\n        <div>\r\n            <el-dialog\r\n                    title=\"添加供应商商品\"\r\n                    :visible.sync=\"dialogVisible\"\r\n                    width=\"30%\">\r\n                <span>\r\n                    <el-row>\r\n                        <el-col :span=\"8\">\r\n                            <div class=\"grid-content bg-purple\">\r\n                                <el-input v-model=\"supply.name\" placeholder=\"请输入供应商名称\">\r\n                                </el-input>\r\n                            </div>\r\n                        </el-col>\r\n                        <el-col :span=\"8\">\r\n                            <div class=\"grid-content bg-purple\">\r\n                                <el-input v-model=\"supply.price\" placeholder=\"请输入价格\">\r\n                                </el-input>\r\n                            </div>\r\n                        </el-col>\r\n                        <el-col :span=