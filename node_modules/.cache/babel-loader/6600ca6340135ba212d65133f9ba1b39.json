{"ast":null,"code":"import OrderPlat from \"@/api/dispatcher/OrderPlat\";\nexport default {\n  name: \"OrderPlat\",\n\n  data() {\n    return {\n      orders: [],\n      // 订单信息\n      page: 1,\n      limit: 5,\n      total: 0,\n      multipleSelection: []\n    };\n  },\n\n  created() {\n    this.pageFindNewOrder();\n  },\n\n  methods: {\n    pageFindNewOrder() {\n      // 分页查找用户新创建的订单\n      OrderPlat.pageDisNewOrder(this.page, this.limit).then(resp => {\n        if (resp.data.code === 200) {\n          this.orders = resp.data.data.data;\n          this.total = resp.data.data.total;\n        }\n      });\n    },\n\n    handleSizeChange(limit) {\n      // 每页数量改变\n      this.limit = limit;\n      this.pageFindNewOrder();\n    },\n\n    handleCurrentChange(page) {\n      // 页数改变\n      this.page = page;\n      this.pageFindNewOrder();\n    },\n\n    handleSelectionChange(val) {\n      // 多选操作\n      this.multipleSelection = [];\n      val.forEach(item => {\n        this.multipleSelection.push(item.id);\n      });\n      this.multipleSelection.forEach(item => {\n        console.log(item);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4EA;AAEA;EACAA,iBADA;;EAEAC;IACA;MACAC,UADA;MACA;MACAC,OAFA;MAGAC,QAHA;MAIAC,QAJA;MAKAC;IALA;EAOA,CAVA;;EAUAC;IACA;EACA,CAZA;;EAYAC;IACAC;MAAA;MACAC;QACA;UACA;UACA;QACA;MACA,CALA;IAMA,CARA;;IASAC;MAAA;MACA;MACA;IACA,CAZA;;IAaAC;MAAA;MACA;MACA;IACA,CAhBA;;IAiBAC;MAAA;MACA;MACAC;QACA;MACA,CAFA;MAGA;QACAC;MACA,CAFA;IAGA;;EAzBA;AAZA","names":["name","data","orders","page","limit","total","multipleSelection","created","methods","pageFindNewOrder","OrderPlat","handleSizeChange","handleCurrentChange","handleSelectionChange","val","console"],"sourceRoot":"src/views/dispatcher","sources":["OrderPlat.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div style=\"height: 60px\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange