ds = []\r\n                orders.forEach(o => {\r\n                    this.selectionIds.push(o.id)\r\n                })\r\n\r\n                this.isMultiple = this.selectionIds.length > 0\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}