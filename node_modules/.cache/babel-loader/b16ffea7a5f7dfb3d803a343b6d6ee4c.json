{"ast":null,"code":"import GoodsTypeApi from \"@/api/goodsType/GoodsTypeApi\";\nimport SupplyApi from \"@/api/supply/SupplyApi\";\nexport default {\n  name: \"GoodType\",\n\n  data() {\n    return {\n      data: [],\n      // 分页数据\n      total: 10,\n      // 总记录数量\n      size: 5,\n      // 一页几条\n      current: 1,\n      dialogVisible: false,\n      goodsType: {\n        id: null,\n        name: '',\n        supplierId: null,\n        number: 0,\n        supplyId: null\n      },\n      updateDialogVisible: false,\n      supplies: []\n    };\n  },\n\n  methods: {\n    inGoods(id) {\n      // 进货\n      GoodsTypeApi.get(id).then(resp => {\n        this.goodsType = resp.data.data;\n      });\n    },\n\n    update() {\n      GoodsTypeApi.update(this.supplier).then(resp => {\n        this.page();\n        this.updateDialogVisible = false;\n      });\n    },\n\n    updateView(id) {\n      GoodsTypeApi.get(id).then(resp => {\n        this.goodsType = resp.data.data;\n        this.updateDialogVisible = true;\n      });\n    },\n\n    addView() {\n      SupplyApi.getAll().then(resp => {\n        this.supplies = resp.data.data;\n      });\n      this.dialogVisible = true;\n    },\n\n    save() {\n      GoodsTypeApi.add(this.goodsType).then(resp => {\n        if (resp.data.code != 200) {\n          this.$message.error(resp.data.msg);\n        } else {\n          this.dialogVisible = false;\n          this.page();\n        }\n      });\n    },\n\n    deleteData(id) {\n      GoodsTypeApi.deleteData(id).then(resp => {\n        if (resp.data.code == 200) {\n          this.$message.success(\"删除成功\");\n          this.page();\n        } else {\n          this.$message.error(resp.data.msg);\n        }\n      });\n    },\n\n    page() {\n      GoodsTypeApi.page(this.current, this.size).then(resp => {\n        this.data = resp.data.data.records;\n        this.total = resp.data.data.total;\n      });\n    },\n\n    handleSizeChange(size) {\n      this.size = size;\n      this.page();\n    },\n\n    handleCurrentChange(current = 1) {\n      this.current = current;\n      this.page();\n    }\n\n  },\n\n  created() {\n    this.page();\n  }\n\n};","map":{"version":3,"mappings":"AA2HA;AACA;AAEA;EACAA,gBADA;;EAEAC;IACA;MACAA,QADA;MACA;MACAC,SAFA;MAEA;MACAC,OAHA;MAGA;MACAC,UAJA;MAKAC,oBALA;MAMAC;QACAC,QADA;QAEAP,QAFA;QAGAQ,gBAHA;QAIAC,SAJA;QAKAC;MALA,CANA;MAaAC,0BAbA;MAcAC;IAdA;EAgBA,CAnBA;;EAoBAC;IACAC;MAAA;MACAC;QACA;MACA,CAFA;IAGA,CALA;;IAMAC;MACAD;QACA;QACA;MACA,CAHA;IAIA,CAXA;;IAYAE;MACAF;QACA;QACA;MACA,CAHA;IAIA,CAjBA;;IAkBAG;MACAC;QACA;MACA,CAFA;MAGA;IACA,CAvBA;;IAwBAC;MACAL;QACA;UACA;QACA,CAFA,MAEA;UACA;UACA;QACA;MACA,CAPA;IAQA,CAjCA;;IAkCAM;MACAN;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA3CA;;IA4CAO;MACAP;QACA;QACA;MACA,CAHA;IAIA,CAjDA;;IAmDAQ;MACA;MACA;IACA,CAtDA;;IAuDAC;MACA;MACA;IACA;;EA1DA,CApBA;;EAgFAC;IACA;EACA;;AAlFA","names":["name","data","total","size","current","dialogVisible","goodsType","id","supplierId","number","supplyId","updateDialogVisible","supplies","methods","inGoods","GoodsTypeApi","update","updateView","addView","SupplyApi","save","deleteData","page","handleSizeChange","handleCurrentChange","created"],"sourceRoot":"src/views/goodsType","sources":["GoodsType.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div>\r\n            <el-button type=\"primary\" @click=\"addView\">添 加</el-button>\r\n        </div>\r\n        <div>\r\n            <template>\r\n                <el-table\r\n                        :data=\"data\"\r\n                        border\r\n                        style=\"width: 100%\">\r\n                    <el-table-column\r\n                            prop=\"id\"\r\n                            label=\"编号\"\r\n                            width=\"200\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            prop=\"name\"\r\n                            label=\"名称\"\r\n                            width=\"200\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            prop=\"supplierId\"\r\n                            label=\"供应商编号\"\r\n                            width=\"200\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            prop=\"supplyId\"\r\n                            label=\"供应品编号\"\r\n                            width=\"200\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            prop=\"number\"\r\n                            label=\"数量\"\r\n                            width=\"200\">\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            label=\"更新\"\r\n                            width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button type=\"info\" @click=\"inGoods(scope.row.id)\">进 货</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column\r\n                            label=\"删除\"\r\n                            width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button type=\"danger\" @click=\"deleteData(scope.row.id)\">删除</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </template>\r\n        </div>\r\n\r\n        <div>\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"current\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"size\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n\r\n        <div>\r\n            <el-dialog\r\n                    title=\"添加商品\"\r\n                    :visible.sync=\"dialogVisible\"\r\n                    width=\"30%\">\r\n                <span>\r\n                     <el-row>\r\n                        <el-col :span=\"12\">\r\n                            <div class=\"grid-content bg-purple\">\r\n                                <el-select v-model=\"goodsType.supplyId\"\r\n                                           placeholder=\"请选择进货源头\">\r\n                                    <el-option\r\n                                            v-for=\"item in supplies\"\r\n                                            :key=\"item.id\"\r\n                                            :label=\"item.name\"\r\n                                            :value=\"item.id\">\r\n                                    </el-option>\r\n                                </el-select>\r\n                            </div>\r\n                        </el-col>\r\n                    </el-row>\r\n                </span>\r\n                <span slot=\"footer\" class=\"dialog-footer\">\r\n                    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"save\">添 加</el-button>\r\n                </span>\r\n            </el-dialog>\r\n        </div>\r\n\r\n        <div>\r\n            <el-dialog\r\n                    title=\"进 货\"\r\n                    :visible.sync=\"dialogVisible\"\r\n                    width=\"30%\">\r\n                <span>\r\n                     <el-row>\r\n                        <el-col :span=\"12\">\r\n                            <div class=\"grid-content bg-purple\">\r\n                                <el-date-picker\r\n                                        v-model=\"goods.overTime\"\r\n                                        type=\"datetime\"\r\n                                        placeholder=\"过期时间\">\r\n                                </el-date-picker>\r\n                            </div>\r\n                        </el-col>\r\n                    </el-row>\r\n                </span>\r\n                <span slot=\"footer\" class=\"dialog-footer\">\r\n                    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"save\">添 加</el-button>\r\n                </span>\r\n            </el-dialog>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import GoodsTypeApi from \"@/api/goodsType/GoodsTypeApi\";\r\n    import SupplyApi from \"@/api/supply/SupplyApi\";\r\n\r\n    export default {\r\n        name: \"GoodType\",\r\n        data() {\r\n            return {\r\n                data: [], // 分页数据\r\n                total: 10, // 总记录数量\r\n                size: 5, // 一页几条\r\n                current: 1,\r\n                dialogVisible: false,\r\n                goodsType: {\r\n                    id: null,\r\n                    name: '',\r\n                    supplierId: null,\r\n                    number: 0,\r\n                    supplyId: null,\r\n                },\r\n                updateDialogVisible: false,\r\n                supplies: [],\r\n            }\r\n        },\r\n        methods: {\r\n            inGoods(id) { // 进货\r\n                GoodsTypeApi.get(id).then(resp => {\r\n                    this.goodsType = resp.data.data;\r\n                })\r\n            },\r\n            update() {\r\n                GoodsTypeApi.update(this.supplier).then(resp => {\r\n                    this.page()\r\n                    this.updateDialogVisible = false;\r\n                })\r\n            },\r\n            updateView(id) {\r\n                GoodsTypeApi.get(id).then(resp => {\r\n                    this.goodsType = resp.data.data\r\n                    this.updateDialogVisible = true\r\n                })\r\n            },\r\n            addView() {\r\n                SupplyApi.getAll().then(resp => {\r\n                    this.supplies = resp.data.data\r\n                })\r\n                this.dialogVisible = true;\r\n            },\r\n            save() {\r\n                GoodsTypeApi.add(this.goodsType).then(resp => {\r\n                    if (resp.data.code != 200) {\r\n                        this.$message.error(resp.data.msg);\r\n                    } else {\r\n                        this.dialogVisible = false;\r\n                        this.page()\r\n                    }\r\n                })\r\n            },\r\n            deleteData(id) {\r\n                GoodsTypeApi.deleteData(id).then(resp => {\r\n                    if (resp.data.code == 200) {\r\n                        this.$message.success(\"删除成功\");\r\n                        this.page()\r\n                    } else {\r\n                        this.$message.error(resp.data.msg);\r\n                    }\r\n                })\r\n            },\r\n            page() {\r\n                GoodsTypeApi.page(this.current, this.size).then(resp => {\r\n                    this.data = resp.data.data.records;\r\n                    this.total = resp.data.data.total;\r\n                })\r\n            },\r\n\r\n            handleSizeChange(size) {\r\n                this.size = size;\r\n                this.page();\r\n            },\r\n            handleCurrentChange(current = 1) {\r\n                this.current = current;\r\n                this.page();\r\n            },\r\n        },\r\n        created() {\r\n            this.page()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}